def factors(n):
    factor_list = []
    for i in range(1,int(n**0.5) + 1):
        if n%i==0:
            factor_list.append(i)
            if n//i != i:
                factor_list.append(n//i)
    return sorted(factor_list)
    
n=int(input())
p=int(input())

factor_list=factors(n)

if p>len(factor_list):
    print(0)
else:
    print(factor_list[p-1])


